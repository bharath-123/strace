. "${srcdir=.}/init.sh"

check_prog nproc
timeout="$(($TIMEOUT_DURATION/10))"
nproc="$((64+$timeout+$(nproc)))"

run_prog "../$NAME" "$timeout" "$nproc"
run_strace -f -qq -enone -esignal=none $args
